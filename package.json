{
  "name": "inquirer-chessboard",
  "version": "1.0.0",
  "engines": {
    "node": ">=18"
  },
  "description": "Inquirer chessboard prompt",
  "main": "./dist/cjs/index.js",
  "typings": "./dist/cjs/types/index.d.ts",
  "files": [
    "dist/**/*"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/FinalDoom/inquirer-chessboard.git"
  },
  "keywords": [
    "answer",
    "answers",
    "ask",
    "base",
    "cli",
    "command",
    "command-line",
    "confirm",
    "enquirer",
    "generate",
    "generator",
    "hyper",
    "input",
    "inquire",
    "inquirer",
    "interface",
    "iterm",
    "javascript",
    "menu",
    "node",
    "nodejs",
    "prompt",
    "promptly",
    "prompts",
    "question",
    "readline",
    "scaffold",
    "scaffolder",
    "scaffolding",
    "stdin",
    "stdout",
    "terminal",
    "tty",
    "ui",
    "yeoman",
    "yo",
    "zsh",
    "chessboard",
    "grid",
    "checkers",
    "spreadsheet",
    "table"
  ],
  "author": "FinalDoom",
  "license": "MIT",
  "homepage": "https://github.com/FinalDoom/inquirer-chessboard",
  "scripts": {
    "tsc": "npm run tsc:esm && npm run tsc:cjs",
    "tsc:esm": "rm -rf dist/esm && tsc -p ./tsconfig.json",
    "tsc:cjs": "rm -rf dist/cjs && tsc -p ./tsconfig.cjs.json && npx renamer --silent --path-element ext --find '/m([jt]s)/' --replace '$1' './dist/cjs/**/*.m?s'",
    "prepare": "npm run tsc",
    "test": ""
  },
  "dependencies": {
    "@inquirer/core": "^8.2.4",
    "@inquirer/type": "^1.5.5",
    "ansi-escapes": "^4.3.2",
    "chalk": "^4.1.2",
    "cli-table": "^0.3.11",
    "figures": "^6.1.0",
    "strip-ansi": "^7.1.0"
  },
  "devDependencies": {
    "@sindresorhus/tsconfig": "^5.1.1",
    "@types/cli-table": "^0.3.4",
    "globby": "^14.0.2",
    "renamer": "^5.0.2",
    "typescript": "^5.7.2"
  },
  "publishConfig": {
    "access": "public"
  },
  "exports": {
    ".": {
      "import": {
        "types": "./dist/esm/types/index.d.mts",
        "default": "./dist/esm/index.mjs"
      },
      "require": {
        "types": "./dist/cjs/types/index.d.ts",
        "default": "./dist/cjs/index.js"
      }
    }
  }
}
